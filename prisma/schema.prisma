generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider  = "postgresql"
  url       = env("DATABASE_URL")
}

model Garment {
  id          String    @id @default(cuid())
  name        String
  category    String
  imageUrl    String
  active      Boolean   @default(true)
  createdAt   DateTime  @default(now())
}